{
  "rules":
  {
    "users":
    {
      "$user_id":
      {
    		".read": "$user_id === auth.uid",
      	".write": "$user_id === auth.uid"
      }
    },


    "models":
    {
      ".read": true,
      ".write": "auth != null",


        "$model_id":
        {
          ".write": "data.child('uid').val() === auth.uid"
        }
    }
  }
}
